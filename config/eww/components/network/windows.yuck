; Window Sections
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

; Network
(defwindow network-window
	:monitor 0
	:geometry	(geometry	:x "-10px"
							:y "40px"
							:anchor "left bottom")
	:vexpand "false"
	:hexpand "false"
(network-window))

; Network - input
(defwindow network-window-input
	:monitor 0
	:focusable "true"
	:geometry	(geometry	:width "280px"
							:anchor "center")
(network-window-input))